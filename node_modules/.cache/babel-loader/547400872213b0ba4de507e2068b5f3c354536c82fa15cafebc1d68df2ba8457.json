></el-checkbox>\n      <el-checkbox label=\"地推活动\" name=\"type\"></el-checkbox>\n      <el-checkbox label=\"线下主题活动\" name=\"type\"></el-checkbox>\n      <el-checkbox label=\"单纯品牌曝光\" name=\"type\"></el-checkbox>\n    </el-checkbox-group>\n  </el-form-item>\n  <el-form-item label=\"特殊资源\">\n    <el-radio-group v-model=\"form.resource\">\n      <el-radio label=\"线上品牌商赞助\"></el-radio>\n      <el-radio label=\"线下场地免费\"></el-radio>\n    </el-radio-group>\n  </el-form-item>\n  <el-form-item label=\"活动形式\">\n    <el-input type=\"textarea\" v-model=\"form.desc\"></el-input>\n  </el-form-item>\n  <el-form-item>\n    <el-button type=\"primary\" @click=\"onSubmit\">立即创建</el-button>\n    <el-button>取消</el-button>\n  </el-form-item>\n</el-form>\n            <el-button style=\"margin-top: 12px;\" @click=\"next\" v-if=\"this.active !== 3\">下一步</el-button>\n        </el-card>\n    </div>\n</template>\n\n<script>\nexport default {\n    data() {\n        return {\n            active: 1,\n        }\n    },\n    methods: {\n        next() {\n            if(this.active === 1 || this.active === 2) {\n                this.active++\n            } else {\n                alert(\"编号错误\")\n            }\n        }\n    }\n}\n</script>\n\n<style scoped>\n\n</style>"],"mappings":"AA8DA;EACAA,KAAA;IACA;MACAC,MAAA;IACA;EACA;EACAC,OAAA;IACAC,KAAA;MACA,SAAAF,MAAA,eAAAA,MAAA;QACA,KAAAA,MAAA;MACA;QACAG,KAAA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}