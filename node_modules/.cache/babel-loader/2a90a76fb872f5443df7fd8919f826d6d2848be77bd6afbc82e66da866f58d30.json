-button>\n        </el-card>\n    </div>\n</template>\n\n<script>\nimport Upload from '../components/Upload.vue'\nimport ParameterSetting from '@/components/ParameterSetting.vue'\n\nexport default {\n    data() {\n        return {\n            active: 2,\n        }\n    },\n    components: {\n        Upload,\n        ParameterSetting\n    },\n    methods: {\n        prev() {\n            if(this.active === 2) {\n                this.active--\n            } else {\n                alert(\"编号错误\")\n            }\n        },\n        next() {\n            if(this.active === 1) {\n                let form = this.$refs.upload.form\n                if(form.JavaList.length !== 0 && form.JarList.length !== 0 && form.name.length >= 3 && form.name.length <= 20) {\n                    this.active++;\n                } else {\n                    this.$message.warning(\"输入内容不符合要求\")\n                }\n            } else if(this.active === 2) {\n                this.active++\n            }else {\n                alert(\"编号错误\")\n            }\n        }\n    }\n}\n</script>\n\n<style scoped>\n\n</style>"],"mappings":"AAqBA,OAAAA,MAAA;AACA,OAAAC,gBAAA;AAEA;EACAC,KAAA;IACA;MACAC,MAAA;IACA;EACA;EACAC,UAAA;IACAJ,MAAA;IACAC;EACA;EACAI,OAAA;IACAC,KAAA;MACA,SAAAH,MAAA;QACA,KAAAA,MAAA;MACA;QACAI,KAAA;MACA;IACA;IACAC,KAAA;MACA,SAAAL,MAAA;QACA,IAAAM,IAAA,QAAAC,KAAA,CAAAC,MAAA,CAAAF,IAAA;QACA,IAAAA,IAAA,CAAAG,QAAA,CAAAC,MAAA,UAAAJ,IAAA,CAAAK,OAAA,CAAAD,MAAA,UAAAJ,IAAA,CAAAM,IAAA,CAAAF,MAAA,SAAAJ,IAAA,CAAAM,IAAA,CAAAF,MAAA;UACA,KAAAV,MAAA;QACA;UACA,KAAAa,QAAA,CAAAC,OAAA;QACA;MACA,gBAAAd,MAAA;QACA,KAAAA,MAAA;MACA;QACAI,KAAA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}